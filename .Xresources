URxvt*inheritPixmap: false
URxvt*tintColor: gray30
URxvt.internalBorder: 0
URxvt.jumpScroll: true
! URxvt.loginShell: true
URxvt.pointerBlank: true
URxvt.saveLines: 4000
URxvt.secondaryScroll: true
URxvt.scrollBar: false
URxvt.scrollTtyKeypress: true
URxvt.scrollWithBuffer: true
URxvt.urlLauncher: /usr/bin/chromium-browser

URxvt.tabbed.tab-bg: 1
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tabbar-fg: 11

!URxvt.scrollBar: true
!URxvt.scrollstyle: plain
!URxvt.scrollColor: #666666
URxvt.loginShell: true

! Make ^G set urgent bit
URxvt.urgentOnBell: true

! rxvt perl extensions setup
! ---------------------------------------------------------------------------
! removing annoying search
!URxvt.perl-ext-common: default,-searchable-scrollback,selection-popup,tabbed
URxvt.perl-ext-common: default,-searchable-scrollback,selection-popup,tabbed

